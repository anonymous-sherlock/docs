{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Adscrush",
  "logo": {
    "dark": "/logo/logo.png",
    "light": "/logo/logo.png"
  },
  "backgroundImage": "/images/background.png",
  "favicon": "/logo/logo.png",
  "colors": {
    "primary": "#0D9373",
    "light": "#07C983",
    "dark": "#0D9373",
    "background": {
      "light": "#ffffff"
    },
    "anchors": {
      "from": "#0D9373",
      "to": "#07C983"
    }
  },
  "modeToggle": {
    "default": "dark"
  },
  "topbarLinks": [
    {
      "name": "Support",
      "url": "mailto:support@adscrush.com"
    }
  ],
  "topbarCtaButton": {
    "name": "Dashboard",
    "url": "https://affilate.adscrush.com/dashboard"
  },
  "tabs": [
    {
      "name": "API Reference",
      "url": "api-reference"
    },
    {
      "name": "Knowledge Base",
      "url": "knowledge-base"
    }
  ],
  "anchors": [
    {
      "name": "Documentation",
      "icon": "book-open-cover",
      "url": "/"
    },
    {
      "name": "Api Reference",
      "icon": "rectangle-terminal",
      "url": "api-reference"
    },
    {
      "name": "Knowledge Base",
      "icon": "books",
      "url": "knowledge-base"
    }
  ],
  "navigation": [
    {
      "group": "Overview",
      "pages": ["introduction", "contributing", "changelog", "roadmap"]
    },
    {
      "group": "Get Started",
      "pages": [
        {
          "group": "Products",
          "pages": ["/products"]
        },
        {
          "group": "Campaigns",
          "pages": ["/campaigns"]
        },
        {
          "group": "Leads",
          "pages": ["/leads"]
        }
      ]
    },

    {
      "group": "API Documentation",
      "pages": ["api-reference/introduction"]
    },
    {
      "group": "Leads endpoints",
      "pages": [
        "api-reference/leads-endpoint/get",
        "api-reference/leads-endpoint/create",
        "api-reference/leads-endpoint/delete"
      ]
    },
    {
      "group": "Knowledge Base",
      "pages": [
        "knowledge-base/introduction",
        "knowledge-base/how-do-i-fix-cors-issues"
      ]
    }
  ],
  "footerSocials": {
    "twitter": "https://twitter.com/mintlify",
    "github": "https://github.com/mintlify",
    "linkedin": "https://www.linkedin.com/company/mintsearch"
  },
  "feedback": {
    "raiseIssue": true,
    "suggestEdit": true,
    "thumbsRating": true
  },
  "api": {
    "baseUrl": "https://mintlify.com/api",
    "auth": {
      "method": "bearer"
    }
  }
}
